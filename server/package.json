{
  "name": "fauna-grocery-server",
  "version": "0.0.1",
  "description": "empty",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "start": "cross-env NODE_ENV=dev nodemon --exec babel-node ./index",
    "build": "npm run clean && babel -d ./build ./ --config-file ./.babelrc",
    "heroku": "npm run build && cross-env NODE_ENV=prod node ./build",
    "test": "cross-env NODE_ENV=test mocha --reporter spec --slow 5000 --require @babel/register tests/integration/**/*.js"
  },
  "engines": {
    "node": "10.12.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thismarcoantonio/fauna-grocery.git"
  },
  "keywords": [
    "fauna-grocery",
    "grocery-list",
    "nodejs"
  ],
  "author": "thismarcoantonio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thismarcoantonio/fauna-grocery/issues"
  },
  "homepage": "https://github.com/thismarcoantonio/fauna-grocery#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "jwt-simple": "^0.5.5",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "sequelize": "^4.41.0",
    "sqlite3": "^4.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-minify": "^0.5.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "eslint": "^5.7.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4"
  }
}
